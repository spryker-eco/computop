parameters:
    checkGenericClassInNonGenericObjectType: false
    checkMissingIterableValueType: false

    ignoreErrors:
        - '#Parameter \#5 \$request of class.+\|null given.#'
        - '#.+Transfer.+#'
        - '#.+but returns.+null.#'
        - '#Call to an undefined method .+LocatorLocatorInterface::.+\(\)#'
        - '#Parameter .+of method|function .+expects int, int\|null given.#'
        - '#Parameter .+of method|function .+expects string, string\|null given.#'

    excludePaths:
        - src/SprykerEco/Zed/Computop/Persistence/Propel
